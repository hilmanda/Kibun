{
  "expo": {
    "name": "Kibun",
    "slug": "Kibun",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/transparent-Logo-Kibun.png",
      "resizeMode": "contain",
      "backgroundColor": "#10BD01"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "softwareKeyboardLayoutMode": "pan",
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "ACCESS_BACKGROUND_LOCATION",
        "FOREGROUND_SERVICE"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/splash-kibun72.png",
        "backgroundColor": "#10BD01"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
